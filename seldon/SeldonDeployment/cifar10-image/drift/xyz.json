{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "xyz",
        "namespace": "seldon",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "cifar10-image",
            "seldon.io/deployment-namespace": "seldon",
            "seldon.io/detector-type": "drift"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "cifar10-image",
                    "seldon.io/drift": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "drift-detector",
                        "args": [
                            "--model_name",
                            "cifar10",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "tensorflow.http",
                            "--storage_uri",
                            "gs://seldon-models/alibi-detect/od/OutlierVAE/cifar10",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.inference.outlier",
                            "--event_source",
                            "io.seldon.serving.seldon-seldondeployment-cifar10-image-outlier",
                            "DriftDetector",
                            "--drift_batch_size",
                            "1001"
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8881
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}