{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "xyz",
        "namespace": "production",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "movie-classifier",
            "seldon.io/deployment-namespace": "production",
            "seldon.io/detector-type": "outlier"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "movie-classifier",
                    "seldon.io/outlier": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "outlier-detector",
                        "image": "seldonio/alibi-detect-server:1.9.1",
                        "args": [
                            "--model_name",
                            "xyz",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "tensorflow.http",
                            "--storage_uri",
                            "gs://seldon-models/alibi-detect/od/OutlierVAE/cifar10",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs.svc.cluster.local:80",
                            "--event_type",
                            "io.seldon.serving.inference.outlier",
                            "--event_source",
                            "io.seldon.serving.xyz",
                            "OutlierDetector",
                            "--drift_batch_size",
                            "20"
                        ],
                        "envFrom": [
                            {
                                "secretRef": {
                                    "name": "alibi-secret"
                                }
                            }
                        ],
                        "resources": {}
                    }
                ]
            }
        }
    },
    "status": {}
}