{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "income-data-drift-detect",
        "namespace": "production",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "income-classifier",
            "seldon.io/deployment-namespace": "production",
            "seldon.io/detector-type": "drift"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "income-classifier",
                    "seldon.io/drift": "true"
                },
                "annotations": {
                    "autoscaling.knative.dev/maxScale": "1",
                    "autoscaling.knative.dev/minScale": "1",
                    "prometheus.io/path": "/v1/metrics",
                    "prometheus.io/port": "8080",
                    "prometheus.io/scrape": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "drift-detector",
                        "image": "sachinmv31/alibi-detect-server:1.11.0-dev",
                        "args": [
                            "--model_name",
                            "income-detector",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "seldon.http",
                            "--storage_uri",
                            "s3://detect/income-data-drift-detector/",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.inference.drift",
                            "--event_source",
                            "io.seldon.serving.income-data-drift-detect",
                            "DriftDetector",
                            "--drift_batch_size",
                            "5"
                        ],
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ],
                        "envFrom": [
                            {
                                "secretRef": {
                                    "name": "alibi-secret"
                                }
                            }
                        ],
                        "env": [
                            {
                                "name": "SELDON_DEPLOYMENT_ID",
                                "value": "income-classifier"
                            },
                            {
                                "name": "SELDON_DEPLOYMENT_NAMESPACE",
                                "value": "production"
                            }
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8888
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}