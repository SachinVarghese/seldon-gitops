{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "income-calssifier",
        "namespace": "production",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "income-classifier",
            "seldon.io/deployment-namespace": "production",
            "seldon.io/detector-type": "outlier"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "income-classifier",
                    "seldon.io/outlier": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "outlier-detector",
                        "image": "seldonio/alibi-detect-server:1.11.0-dev",
                        "args": [
                            "--model_name",
                            "income-calssifier",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "tensorflow.http",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.inference.outlier",
                            "--event_source",
                            "io.seldon.serving.income-calssifier",
                            "OutlierDetector",
                            "--drift_batch_size",
                            "1000"
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8888
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}